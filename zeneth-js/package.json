    "prettier": "prettier --write .",
    "watch": "tsc --watch",
    "build": "tsc --build",
    "clean": "rimraf build coverage .nyc_output cache",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "precommit": "lint-staged"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@ethereumjs/common": "^2.2.0",
