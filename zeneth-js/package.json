    "ethers": "^5.1.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^6.0.5",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0"
